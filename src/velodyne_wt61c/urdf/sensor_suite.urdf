<robot name="sensor_suite">

  <link name="base_link" />

  <link name="wt61c_uart" />

  <link name="velodyne" />

  <joint name="wt61c_uart_joint" type="fixed">
    <parent link="base_link" />
    <child link="wt61c_uart" />
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint>

  <joint name="velodyne_joint" type="fixed">
    <parent link="base_link" />
    <child link="velodyne" />
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint>

</robot>
